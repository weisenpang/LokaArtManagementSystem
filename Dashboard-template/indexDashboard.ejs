    <style>    
        .icons {
            display: flex;
            align-items: center;
            gap: 15px;
        }
        
        /* Profile Content Styles */
        .profile-content {
            display: none;
        }
        
        /* Active state for dashboard content */
        .dashboard-content.active {
            display: block;
        }
        
        .profile-content.active {
            display: block;
        }
        
        /* Profile Page Styles based on screenshot */
        .profile-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 24px;
        }
        
        .profile-header h4 {
            margin: 0;
            color: #566a7f;
        }
        
        .profile-card {
            background: #fff;
            border-radius: 8px;
            box-shadow: 0 2px 6px rgba(0,0,0,0.1);
            margin-bottom: 24px;
        }
        
        .profile-card-header {
            padding: 16px 24px;
            border-bottom: 1px solid #d9dee3;
            font-weight: 600;
            color: #566a7f;
        }
        
        .profile-card-body {
            padding: 24px;
        }
        
        .profile-avatar-section {
            display: flex;
            align-items: center;
            margin-bottom: 24px;
        }
        
        .profile-avatar {
            width: 100px;
            height: 100px;
            border-radius: 50%;
            margin-right: 24px;
            border: 4px solid #e0e0e0;
        }
        
        .profile-avatar-upload {
            display: flex;
            flex-direction: column;
        }
        
        .profile-avatar-upload button {
            margin-bottom: 8px;
        }
        
        .profile-avatar-upload p {
            margin: 0;
            font-size: 0.875rem;
            color: #6c757d;
        }
        
        .profile-form-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 20px;
        }
        
        .profile-form-group {
            margin-bottom: 20px;
        }
        
        .profile-form-group label {
            display: block;
            margin-bottom: 8px;
            font-weight: 500;
            color: #566a7f;
        }
        
        .profile-form-group input,
        .profile-form-group select {
            width: 100%;
            padding: 10px 12px;
            border: 1px solid #d9dee3;
            border-radius: 6px;
            font-size: 15px;
            transition: border-color 0.3s;
        }
        
        .profile-form-group input:focus,
        .profile-form-group select:focus {
            border-color: #4b6cb7;
            outline: none;
        }
        
        .profile-form-actions {
            display: flex;
            justify-content: flex-end;
            gap: 10px;
            margin-top: 20px;
            padding-top: 20px;
            border-top: 1px solid #d9dee3;
        }
        
        .btn-primary {
            background-color: #4b6cb7;
            color: white;
            border: none;
            padding: 10px 20px;
            border-radius: 6px;
            font-weight: 500;
            cursor: pointer;
            transition: all 0.3s;
        }
        
        .btn-primary:hover {
            background-color: #3a5a9f;
        }
        
        .btn-secondary {
            background-color: #f0f0f0;
            color: #566a7f;
            border: 1px solid #d9dee3;
            padding: 10px 20px;
            border-radius: 6px;
            font-weight: 500;
            cursor: pointer;
            transition: all 0.3s;
        }
        
        .btn-secondary:hover {
            background-color: #e4e4e4;
        }
        
        .back-to-dashboard {
            margin-bottom: 20px;
            cursor: pointer;
            display: inline-flex;
            align-items: center;
            color: #4b6cb7;
            font-weight: 500;
        }
        
        .back-to-dashboard:hover {
            color: #3a5a9f;
        }
        
        @media (max-width: 768px) {
            .profile-form-grid {
                grid-template-columns: 1fr;
            }
            
            .profile-avatar-section {
                flex-direction: column;
                text-align: center;
            }
            
            .profile-avatar {
                margin-right: 0;
                margin-bottom: 16px;
            }
        }
    </style>


